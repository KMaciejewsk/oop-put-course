#include <iostream>
#include <string>
using namespace std;

class krzeslo{
	public:
		int ile_nog;
		bool czy_wygodne;
};

class stanowisko{
	public:
		int numer;
		string nazwa;
		int PC_nr;
};

class pc{
	public:
		int nr_ser;
		bool czy_sprawny;
		string cpu;
};

int main() {
	krzeslo krzeslo11;
	stanowisko LAB_GE_11;
	pc PC_11;
	
	krzeslo11.ile_nog = 4;
	krzeslo11.czy_wygodne = 0;
	
	LAB_GE_11.numer = 11;
	LAB_GE_11.nazwa = "LAB_GE_11";
	LAB_GE_11.PC_nr = 11;
	
	PC_11.czy_sprawny = 1;
	PC_11.cpu = "i5";
	PC_11.nr_ser = 11;
	
	cout<<"Stanowisko: " << LAB_GE_11.nazwa <<"\n";
  cout<<"itd...";
	
	return 0;
}
